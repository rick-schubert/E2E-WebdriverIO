{
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "indent": "off",
        "linebreak-style": [
            0,
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console": [
            0
        ],
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ]
    },
    "plugins": [
    ]
}